<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>SRParser</PackageId>
        <Version>1.0.0</Version>
        <Authors>Your Name</Authors>
        <Description>A .NET library for parsing DICOM Structured Report (SR) files and converting them to JSON format</Description>
        <PackageTags>dicom;structured-report;medical;json;parser</PackageTags>
        <RepositoryUrl>https://github.com/your-username/SRParser</RepositoryUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="fo-dicom" Version="5.1.1" />
    </ItemGroup>

</Project>
